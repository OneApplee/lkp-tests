pkgname=hackbench
pkgver=git
pkgrel=0
arch=('i386' 'x86_64' 'riscv64' 'aarch64')
url="https://git.kernel.org/pub/scm/utils/rt-tests/rt-tests"
license=('GPL')
source=("https://git.kernel.org/pub/scm/utils/rt-tests/rt-tests.git")
sha256sums=('SKIP')

package() {
	mkdir -p "${pkgdir}/lkp/benchmarks/${pkgname}"
	cd "$srcdir/rt-tests"
	make install DESTDIR="${pkgdir}/lkp/benchmarks/${pkgname}"
}
